    "file-saver": "^2.0.5",
    "jose": "^5.9.6",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.546.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.2",
    "next": "14.2.18",
    "nodemailer": "^7.0.9",
    "pizzip": "^3.2.0",
    "qrcode": "^1.5.4",
